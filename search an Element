import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[] arr = new int[n];
        for(int i=0;i<n;i++){
            arr[i]=sc.nextInt();
            
        }
        int a = sc.nextInt();
        int flag = 0;
        for(int i=0;i<n;i++){
            if(arr[i]==a){
                System.out.println(a +" is presented in an array.");
                flag = 1;
                break;
            }
            
        }
        if(flag!=1){
            System.out.println(a +" is not presented in an array.");
        }
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
}
